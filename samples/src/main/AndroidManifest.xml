<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mehdi.sakout.fancybuttons.samples" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="mehdi.sakout.fancybuttons.samples.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mehdi.sakout.fancybuttons.samples.XmlButtons"
            android:label="@string/title_activity_xml_buttons" >
        </activity>
        <activity
            android:name="mehdi.sakout.fancybuttons.samples.ProgramButtons"
            android:label="@string/title_activity_program_buttons" >
        </activity>
    </application>

</manifest>
